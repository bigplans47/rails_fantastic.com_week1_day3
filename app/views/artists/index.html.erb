<div class="jumbotron">
  <h1>Artists</h1>
</div>
<p><%= link_to "Make new account", new_user_registration_path%></p>
<%= current_user %>
<%= current_user.email %>
<%= image_tag "dog.jpeg" %>

<ul>
  <% @artists.each do |single_artist| %>
    <li><%= link_to single_artist.name, artist_path(single_artist) %> | <%= single_artist.genre %></li>
  <% end %>
</ul>

<p><%= link_to "Add New Artist", new_artist_path %></p>
